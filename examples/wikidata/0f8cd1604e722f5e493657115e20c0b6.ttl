<https://www.wikidata.org/#query-0f8cd1604e722f5e493657115e20c0b6> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT * 
WHERE 
{
  VALUES ?item {wd:Q3998161} # specify the QIds of interest
  ?item ?ppp ?qqq.           # ask for all triples where the QId is the subject
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""";
