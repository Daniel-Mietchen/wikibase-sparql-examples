WHERE 
{
  VALUES ?item {wd:Q241}
  ?item p:P31 ?statement .   # has a P31 statement
  ?statement ps:P31 wd:Q6256 
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""";
