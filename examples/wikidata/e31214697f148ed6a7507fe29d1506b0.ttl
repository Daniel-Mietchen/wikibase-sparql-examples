<https://www.wikidata.org/#query-e31214697f148ed6a7507fe29d1506b0> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Louisecrow/sandbox/data_pipeline_developer_example>,
    <https://www.wikidata.org//wiki/User:Louisecrow/sandbox/data_pipeline_developer_1>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  schema:target <https://query.wikidata.org/sparql>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
PREFIX pq: <http://www.wikidata.org/prop/qualifier/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT ?itemLabel ?start ?end
WHERE 
{
  ?item p:P39 ?ps.
  ?ps ps:P39 wd:Q14211 .
  OPTIONAL { ?ps pq:P580 ?start }
  OPTIONAL { ?ps pq:P582 ?end }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}""" .
