<https://www.wikidata.org/#query-343ac6a09193797265499833322fbaf1> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:WikiProject_Rivers>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  schema:target <https://query.wikidata.org/sparql>;
  sh:select """PREFIX geof: <http://www.opengis.net/def/function/geosparql/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
PREFIX pq: <http://www.wikidata.org/prop/qualifier/>
PREFIX p: <http://www.wikidata.org/prop/>
#Rivers where the source and mouth are more than 3500 km apart
SELECT * WHERE {
  ?river wdt:P31/wdt:P279* wd:Q355304 .
  ?river p:P625 [ ps:P625 ?coord1 ; pq:P518 wd:Q7376362 ] .
  ?river p:P625 [ ps:P625 ?coord2 ; pq:P518 wd:Q1233637 ] .
  BIND(geof:distance(?coord1, ?coord2) AS ?dist).
  FILTER(?dist > 3500 ).
}""" .
