#defaultView:Map
SELECT ?ville ?villeLabel ?coord ?directionLabel WHERE {
  VALUES ?route { wd:Q1624187 } .
  ?route p:P609 ?s .
  ?s ps:P609 ?ville .
  ?ville wdt:P625 ?coord .
