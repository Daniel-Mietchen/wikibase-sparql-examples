<https://www.wikidata.org/#query-ac4f69b9202555aca0e0ab94c009a891> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#defaultView:Map
SELECT DISTINCT ?item ?itemLabel ?coor
WHERE { 
  {
    SELECT ?item WHERE {
      ?item wdt:P31/wdt:P279* wd:Q473972 .
    }
  }
  ?item wdt:P131* wd:Q22048 .
  OPTIONAL { ?item wdt:P625 ?coor. }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\" }
}""";
  rdfs:comment "TODO"@en;
