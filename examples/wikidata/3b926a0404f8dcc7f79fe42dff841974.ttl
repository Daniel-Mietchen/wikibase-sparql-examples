<https://www.wikidata.org/#query-3b926a0404f8dcc7f79fe42dff841974> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#Locations of universities in Portugal
#added before 2016-10
#defaultView:Map
SELECT ?universityLabel ?universityDescription ?website ?coord WHERE {
  ?university (wdt:P31/wdt:P279*) wd:Q3918.
  ?university wdt:P17 wd:Q45.
  ?university wdt:P625 ?coord.
  OPTIONAL { ?university wdt:P856 ?website. }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en,de\". }
}""";
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:WikiProject_University_degrees/Reports/Portugal>,
    <https://www.wikidata.org//wiki/Wikidata:WikiProject_University_degrees/Queries>;
