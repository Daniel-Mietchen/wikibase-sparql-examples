<https://www.wikidata.org/#query-596c9f9bf9ad206de9ce5233cb2dab82> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Wikidata_curricula/Activities/Explore_some_SPARQL_queries_to_check_data_quality>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  schema:target <https://query.wikidata.org/sparql>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#People with a P496 (ORCID ID) statement but no P106 (occupation) statement
SELECT DISTINCT ?author #?authorLabel (COUNT(?paper) AS ?count)
WHERE
{
    ?author wdt:P496 ?orcid ; 
            wdt:P31 wd:Q5 .
    ?paper wdt:P50 ?author .
    MINUS { ?author wdt:P106 ?occupation . }
#	SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\" }
}
#GROUP BY ?author ?authorLabel
#ORDER BY DESC(?count)
LIMIT 200""" .
