    ?birth wdt:P17 ?country.
  }
  ?item wdt:P31 ?Q5.
  FILTER(EXISTS {
    ?item (wdt:P106/(wdt:P279*)) wd:Q2500638.
    hint:Prior hint:gearing \"forward\".
  })
}""",
