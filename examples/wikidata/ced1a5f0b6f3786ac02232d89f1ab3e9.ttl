  OPTIONAL { ?item rdfs:label ?Label_fr . FILTER (lang(?Label_fr) = \"fr\") }
  OPTIONAL { ?item rdfs:label ?Label_it . FILTER (lang(?Label_it) = \"it\") }
}""" .
