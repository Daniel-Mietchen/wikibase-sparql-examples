<https://www.wikidata.org/#query-470e8973751d8d2cf2e0e10119831140> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?itemLabel ?art ?regio
WHERE {
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"de,en\". }
  {?item wdt:P31 wd:Q41762994;
  OPTIONAL {?item wdt:P1388 ?regio}.
  BIND(\"GVV\" as ?art).}
  union
  {?item wdt:P31 wd:Q1561418;
  OPTIONAL {?item wdt:P1388 ?regio}.
  BIND(\"VVG\" as ?art).}
}""";
