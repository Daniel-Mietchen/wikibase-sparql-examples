<https://www.wikidata.org/#query-28e1d1ab382d8604d9c202590767da37> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Julia.athen>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  schema:target <https://query.wikidata.org/sparql>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#defaultView:BubbleChart
SELECT ?venue ?venueLabel (COUNT(?venue) as ?count)
WHERE 
{
  VALUES ?authors { wd:Q63809637 wd:Q4484888 wd:Q63759170 wd:Q63808847 }
  ?work wdt:P50 ?authors.
  ?work wdt:P1433 ?venue.

  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
GROUP BY ?venue ?venueLabel
ORDER BY DESC (?count)""" .
