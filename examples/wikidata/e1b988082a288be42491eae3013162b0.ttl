<https://www.wikidata.org/#query-e1b988082a288be42491eae3013162b0> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """Select ?item ?itemLabel ?location ?locationLabel

where { ?item wdt:P195 wd:Q674773 .
       
       
       OPTIONAL { ?item wdt:P276 ?location }
       
       SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
       
       }""";
