<https://www.wikidata.org/#query-dfde96f0abd46b03fc27539b371c0c69> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#defaultView:Map
SELECT DISTINCT ?item ?itemLabel ?pic ?coord ?GBIFlink WHERE {
  ?item wdt:P8714 ?GADM .
  ?item wdt:P625 ?coord .
  OPTIONAL { ?item wdt:P18 ?pic . }
  BIND(URI(CONCAT(\"https://www.gbif.org/occurrence/map?gadm_gid=\", STR(?GADM))) AS ?GBIFlink)
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""";
