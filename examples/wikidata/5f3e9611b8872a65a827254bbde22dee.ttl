<https://www.wikidata.org/#query-5f3e9611b8872a65a827254bbde22dee> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#Occupations of Santal people (ethnicity)
#defaultView:BubbleChart
SELECT DISTINCT ?occupationLabel (COUNT(*) AS ?count) WHERE {
  ?person wdt:P31 wd:Q5;
    wdt:P172 wd:Q1260346.
  OPTIONAL { ?person wdt:P106 ?occupation. }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
GROUP BY ?occupationLabel""";
