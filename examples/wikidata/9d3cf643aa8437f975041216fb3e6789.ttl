@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-9d3cf643aa8437f975041216fb3e6789> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?itemLabel ?birthdate ?deathdate ?nationalityLabel ?occupationLabel ?itemDescription WHERE {
  ?item wdt:P31 wd:Q5;
    wdt:P27 wd:Q31.
  FILTER(?deathdate < \"1830-01-01T00:00:00Z\"^^xsd:dateTime)
  OPTIONAL { ?item wdt:P734 ?lastname. }
  OPTIONAL { ?item wdt:P569 ?birthdate. }
  OPTIONAL { ?item wdt:P570 ?deathdate. }
  OPTIONAL { ?item wdt:P27 ?nationality. }
  OPTIONAL { ?item wdt:P106 ?occupation. }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"nl,en,fr,de,es,it\". }
}
ORDER BY (?itemLabel)""";
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Geertivp/training/Wikidata_Query/Died_before_Belgium>;
