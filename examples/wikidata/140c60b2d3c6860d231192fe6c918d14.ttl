<https://www.wikidata.org/#query-140c60b2d3c6860d231192fe6c918d14> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT DISTINCT ?hospitalLabel  ?affiliation ?affiliationLabel
WHERE
{
  ?hospital wdt:P31/wdt:P279* wd:Q16917; #hospital          
            wdt:P131 wd:Q1711; #Potsdam
            wdt:P1416 ?affiliation.
  ?affiliation wdt:P279* wd:Q9174.

  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}
ORDER BY ?hospitalLabel""";
