{ 
	?item wdt:P106 wd:Q1930187 .
	?item wdt:P2002 ?user . 
	OPTIONAL { ?item wdt:P27 ?nat . ?nat wdt:P625 ?coor }
	?item wdt:P31 wd:Q5 .
	SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
	wd:P2002 wdt:P1630 ?f .
}""";
