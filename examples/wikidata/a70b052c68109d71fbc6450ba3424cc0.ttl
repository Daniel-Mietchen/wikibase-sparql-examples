<https://www.wikidata.org/#query-a70b052c68109d71fbc6450ba3424cc0> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#defaultView:Map
PREFIX psv: <http://www.wikidata.org/prop/statement/value/>
SELECT ?orgLabel ?hqLabel ?coord ?lat ?lon WHERE {
  ?org wdt:P463 wd:Q5275906.
  ?org wdt:P159 ?hq.
  OPTIONAL { ?hq wdt:P625 ?coord. }
  OPTIONAL {
    ?hq p:P625 ?statement.
    ?statement psv:P625 ?node.
    ?node wikibase:geoLatitude ?lat.
    ?node wikibase:geoLongitude ?lon.
  }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}""";
