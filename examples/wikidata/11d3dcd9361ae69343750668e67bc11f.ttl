<https://www.wikidata.org/#query-11d3dcd9361ae69343750668e67bc11f> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#TEMPLATE={ \"template\": { \"en\": \"Literary works 'from' country ?country\" }, \"variables\": { \"?country\": { \"query\": \"SELECT ?id WHERE { ?id wdt:P31 wd:Q6256. }\" } } }
SELECT DISTINCT ?item WHERE {
  VALUES ?country {wd:Q40 wd:Q39 wd:Q183}
  ?item wdt:P31 wd:Q7725634.
  { # Author was born in ?country
    ?item wdt:P50 ?author.
    ?author wdt:P19 ?place.
    ?place wdt:P17 ?country.
  }
  UNION
  { # Author has ?country citizenship
    ?item wdt:P50 ?author.
    ?author wdt:P27 ?country.
  }
  UNION
  { # First performance was in ?country
    ?item wdt:P4647 ?firstperformance.
    ?firstperformance wdt:P17 ?country.
  }
}""";
