<https://www.wikidata.org/#query-05ee58dddcb1554dac230b844eadd79b> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Request_a_query/Archive/2022/03>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  schema:target <https://query.wikidata.org/sparql>;
  sh:select """PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
SELECT DISTINCT ?museum WHERE
{
   { SELECT ?museum WHERE 
  {
    ?museum wdt:P131* wd:Q1356.
   } }  ?museum wdt:P31/wdt:P279* wd:Q33506.           
}""";
  <http://example.org/bigdata_select> """PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
SELECT DISTINCT ?museum WITH { SELECT ?museum WHERE 
  {
    ?museum wdt:P131* wd:Q1356.
   } } as %i
WHERE
{
  INCLUDE %i
  ?museum wdt:P31/wdt:P279* wd:Q33506. hint:Prior hint:gearing \"forward\".          
}""" .
