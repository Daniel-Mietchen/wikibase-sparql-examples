<https://www.wikidata.org/#query-5b04f132581935afb0d9d51f1c806282> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#TEMPLATE={ \"template\": { \"en\": \"Video games 'from' country ?country\" }, \"variables\": { \"?country\": { \"query\": \"SELECT ?id WHERE { ?id wdt:P31 wd:Q6256. }\" } } }
SELECT DISTINCT ?item WHERE {
  VALUES ?country {wd:Q40 wd:Q39 wd:Q183}
  ?item (wdt:P31/(wdt:P279*)) wd:Q7889.
  { # Country of origin is ?country
    ?item wdt:P495 ?country.
  }
  UNION
  { # Developer is from ?country
    ?item wdt:P178 ?developer.
    ?developer wdt:P17 ?country.
  }
}""";
